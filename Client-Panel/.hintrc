{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "outline-color: color-mix(in oklab, var(--color-ring) 50%, transparent)",
          "scrollbar-width",
          "scrollbar-width: none",
          "-webkit-mask-image",
          "user-select"
        ]
      }
    ],
    "typescript-config/consistent-casing": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ]
  }
}